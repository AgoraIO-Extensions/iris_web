{
  "name": "terra-generate",
  "version": "1.0.0",
  "description": "generate code by terra",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "terra-cli",
    "lint": "eslint -c ../../packages/@iris/rtc/.eslintrc --ext ../../packages/@iris/rtc/generate && prettier --check ../../packages/@iris/rtc/generate",
    "lint:fix": "eslint -c ../../packages/@iris/rtc/.eslintrc --fix --ext ../../packages/@iris/rtc/generate && prettier --write ../../packages/@iris/rtc/generate",
    "postinstall": "pnpm link terra/terra-cli",
    "terra": "sh run-terra.sh"
  },
  "devDependencies": {
    "terra": "../../../terra",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
